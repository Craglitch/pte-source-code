---
import BaseHead from '../components/BaseHead.astro';
import Footer from '../components/Footer.astro';
import Header from '../components/Header.astro';
import { SITE_DESCRIPTION, SITE_TITLE } from '../consts';
---

<!doctype html>
<html lang="en">
  <head>
    <meta property="og:site_name" content="Link preview site name">
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
    <Header /> <!-- src src/components/Header.astro --> 
    <main>
      <div>
        <h1 style="text-align: center"><span  style="color:orange">JPTE</span> SMK <span>Penanti</span>  </h1>
      </div>
      <hr>
      <div style="margin:20px">
        <div class="flex-1">
          <img src="finalTulisanWarnaHItam.png" alt="logo smk pennti">
        </div>
        <div class="flex-2">
          <h1 style="text-align: center"> SESI 2025/2026</h1>
          <img src="2025.jpg" alt="2025 pte penanti"> 
        </div>
      </div>
    		</main>
        <Footer /> <!-- src/components/Footer.astro -->
	</body>
</html>
